meta {
  name: Delete_Workspace_Document
  type: http
  seq: 3
}

delete {
  url: {{baseUrl}}/api/workspaces/{{workspaceId}}/documents/{{documentId}}
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
  res.body.message: isDefined
}

tests {
  test("should delete document successfully", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody().message).to.contain("deleted");
  });
}

docs {
  Delete a document from a workspace
}