meta {
  name: Upload_Document_Success
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/workspaces/{{workspaceId}}/documents/upload
  body: multipartForm
  auth: bearer
}

form {
  file: @/path/to/test.txt
}

assert {
  res.status: eq 201
  res.body.message: isDefined
  res.body.document.filename: eq "test.txt"
  res.body.document.processing_status: eq "pending"
}

docs {
  Upload a document to a workspace
}