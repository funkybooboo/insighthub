meta {
  name: List Documents Success
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/documents
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
  res.body.documents: isArray
}

tests {
  test("should return list of documents", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody().documents).to.be.an("array");
  });
}
