meta {
  name: Get Current User
  type: http
  seq: 7
}

get {
  url: {{baseUrl}}/api/auth/me
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
  res.body.username: isDefined
}

tests {
  test("should return current user", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody().username).to.be.a("string");
  });
}
