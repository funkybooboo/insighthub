meta {
  name: Login Invalid Credentials
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "username": "testuser",
    "password": "wrongpassword"
  }
}

assert {
  res.status: eq 401
}

tests {
  test("should return 401 for invalid credentials", function() {
    expect(res.getStatus()).to.equal(401);
  });
}
