meta {
  name: Health Check
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/health
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.status: eq healthy
}

tests {
  test("should return healthy status", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody().status).to.equal("healthy");
  });
}
