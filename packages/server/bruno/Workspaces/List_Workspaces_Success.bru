meta {
  name: List Workspaces Success
  type: http
  seq: 5
}

get {
  url: {{baseUrl}}/api/workspaces
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
  res.body: isArray
}

tests {
  test("should return list of workspaces", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody()).to.be.an("array");
  });
}
