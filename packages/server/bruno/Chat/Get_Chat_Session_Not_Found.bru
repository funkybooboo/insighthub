meta {
  name: Get Chat Session Not Found
  type: http
  seq: 8
}

get {
  url: {{baseUrl}}/api/workspaces/1/chat/sessions/99999
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 404
  res.body.error: isDefined
}

tests {
  test("should return 404 for non-existent session", function() {
    expect(res.getStatus()).to.equal(404);
    expect(res.getBody().error).to.be.a("string");
  });
}