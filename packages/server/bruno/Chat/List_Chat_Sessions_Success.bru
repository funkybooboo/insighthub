meta {
  name: List Chat Sessions Success
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/workspaces/1/chat/sessions
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
  res.body: isArray
}

tests {
  test("should return array of chat sessions", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody()).to.be.an("array");
  });
}