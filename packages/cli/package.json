{
  "name": "insighthub-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "insighthub-cli": "dist/cli.js"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "dev": "bun run src/cli.ts",
    "build:ts": "tsc",
    "build:pkg": "pkg dist/cli.js --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output bin/insighthub-cli",
    "build": "bun run build:ts && bun run build:pkg"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
