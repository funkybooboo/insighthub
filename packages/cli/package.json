{
    "name": "insighthub-cli",
    "version": "0.1.0",
    "type": "module",
    "bin": {
        "insighthub-cli": "dist/cli.js"
    },
    "scripts": {
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
        "format:check": "prettier --check 'src/**/*.{ts,tsx,js,json,md}'",
        "dev": "bun run src/cli.ts",
        "build:ts": "tsc",
        "build:pkg": "pkg dist/cli.js --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output bin/insighthub-cli",
        "build": "bun run build:ts && bun run build:pkg",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/fs-extra": "^11.0.4",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^24.10.1",
        "@vitest/ui": "^1.3.1",
        "eslint": "^9.39.1",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.5.0",
        "pkg": "^5.8.1",
        "prettier": "^3.6.2",
        "typescript": "^5",
        "typescript-eslint": "^8.47.0",
        "vitest": "^1.3.1"
    },
    "dependencies": {
        "@insighthub/shared-typescript": "workspace:*",
        "axios": "^1.13.2",
        "commander": "^12.1.0",
        "inquirer": "^10.2.2",
        "chalk": "^5.3.0",
        "ora": "^8.1.0",
        "cli-table3": "^0.6.5",
        "fs-extra": "^11.2.0",
        "dotenv": "^16.4.5",
        "mime-types": "^2.1.35"
    }
}
